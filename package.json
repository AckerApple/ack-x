{
  "name": "ack-x",
  "version": "1.4.0",
  "description": "Javascript variable exposing library",
  "main": "js/index.js",
  "typings": "js/index.d.ts",
  "scripts": {
    "test": "mocha ./test/cases --recursive",
    "test:watch": "mocha ./test/cases --recursive --watch",
    "start": "webpack-dev-server --content-base test/",
    "watch": "webpack --watch --progress",
    "build": "npm-run-all build:js build:dist",
    "build:dist": "webpack --progress",
    "build:js": "tsc ts/* --outDir js --declaration true"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/AckerApple/ack-x.git"
  },
  "keywords": [
    "expose",
    "javascript exposing"
  ],
  "author": "Acker Apple",
  "license": "MIT",
  "dependencies": {
    "ack-p": "^1.0.4",
    "debug": "^3.1.0",
    "moment": "^2.20.1"
  },
  "devDependencies": {
    "mocha": "^4.0.1",
    "npm-run-all": "^4.1.2",
    "typescript": "^2.5.3",
    "uglifyjs-webpack-plugin": "^1.2.5",
    "webpack": "^4.6.0",
    "webpack-cli": "^2.0.15"
  }
}
